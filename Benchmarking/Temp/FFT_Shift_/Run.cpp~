#include "iB_FFTShift.h"

int main()
{
	iB_FFTShift::FFTShift_2D_CPU(8, 8);
	iB_FFTShift::FFTShift_2D_CUDA(8, 8);

	iB_FFTShift::FFTShift_3D_CPU(4, 4, 4);
	iB_FFTShift::FFTShift_3D_CUDA(4,4, 4);

	return 0;
}
